{
  "name": "@iamproperty/components",
  "description": "Component library for iamproperty",
  "author": {
    "name": "iamproperty"
  },
  "private": false,
  "version": "7.1.0--beta5",
  "scripts": {
    "temp": "rollup --environment COMPONENT:carousel --config rollup-component.config.js --sourcemap",
    "bootstrap": "copyfiles -u 2 node_modules/bootstrap/**/* assets/bootstrap",
    "reload": "vite --host",
    "dev": "npm run copy && node local_modules/delete-assets.js && vite --host",
    "build": "node local_modules/delete-build-assets.js && npm run compile && vue-tsc --noEmit && vite build --mode lib",
    "prepack": "npm run build",
    "build:docs": "node local_modules/delete-build-assets.js && npm run compile && npm run copy && vue-tsc --noEmit && vite build",
    "preview": "vite preview --host",
    "compile:sass": "sass assets/sass/main.scss assets/css/style.min.css --style=compressed && sass assets/sass/core.scss assets/css/core.min.css --style=compressed && sass assets/sass/components:assets/css/components --style=compressed",
    "compile:js": "rollup --environment BUNDLE:true --config rollup.config.js --sourcemap && terser --compress passes=2 --mangle --comments \"/^!/\" --source-map \"content=assets/js/scripts.bundle.js.map,includeSources,url=scripts.bundle.min.js.map\" --output assets/js/scripts.bundle.min.js assets/js/scripts.bundle.js",
    "compile:ts": "node node_modules/typescript/bin/tsc --project tscompileconfig.json && fix-esm-import-path assets/js",
    "compile": "npm run compile:sass && npm run compile:ts && npm run compile:js",
    "watch:ts": "tsc-watch -p tscompileconfig.json --onCompilationComplete \"fix-esm-import-path assets/js\"",
    "watch:sass": "sass --watch assets/sass/main.scss:assets/css/style.min.css assets/sass/core.scss:assets/css/core.min.css assets/sass/error.scss:assets/css/error.min.css assets/sass/components:assets/css/components assets/sass/components.reset.scss:assets/css/components.reset.min.css --style=compressed",
    "watch": "concurrently \"npm run dev\" \"npm run watch:ts\" \"npm run watch:sass\" \"node local_modules/components.js\"",
    "watch2": "npm run watch:ts && npm run watch:sass && node local_modules/components.js && npm run dev",
    "copy:svg": "copyfiles -u 2 assets/svg/**/* public/svg && copyfiles -u 2 assets/svg/icons.svg public/svg && copyfiles -u 2 assets/svg/logo.svg public/svg",
    "copy:img": "copyfiles -u 2 assets/img/* public/img && copyfiles -u 2 assets/img/**/* public/img && copyfiles -u 2 assets/img/**/**/* public/img",
    "copy:fonts": "copyfiles -u 2 assets/fonts/**/* public/fonts",
    "copy:icons": "copyfiles -u 2 assets/favicons/**/* public/",
    "copy:css": "copyfiles -u 2 assets/css/**/* public/assets/css && copyfiles -u 2 assets/css/* public/assets/css",
    "copy:js": "copyfiles -u 2 assets/js/**/* public/assets/js",
    "copy": "npm run copy:svg && npm run copy:img && npm run copy:fonts && npm run copy:icons && npm run copy:css && npm run copy:js",
    "audit": "node local_modules/audit.js",
    "gen:email": "sass assets/sass/email.scss assets/css/email.min.css --style=compressed && node local_modules/email-css.js",
    "gen:error": "sass assets/sass/error.scss assets/css/error.min.css --style=compressed && node local_modules/error-css.js",
    "test:vue": "jest src",
    "test:unit": "jest assets/ts/tests",
    "test:scss": "jest assets/sass/_tests",
    "test": "npm run test:unit && npm run test:scss",
    "pull-request": "npm run test && npm run compile && npm run audit",
    "deploy:fonts": "copyfiles -u 2 assets/fonts/**/* public/assets/fonts",
    "deploy:svg": "copyfiles -u 2 assets/svg/icons.svg public/svg && copyfiles -u 2 assets/svg/logo.svg public/svg",
    "deploy:404": "node local_modules/create404.js",
    "lint": "eslint assets/ts/scripts.ts assets/ts/components.ts assets/ts/components/**/*.ts assets/ts/modules/**/*.ts",
    "lint:fix": "eslint assets/ts/scripts.ts assets/ts/components.ts assets/ts/components/**/*.ts assets/ts/modules/**/*.ts --fix",
    "prettier": "prettier assets/ts assets/sass src docs --write"
  },
  "dependencies": {
    "@eslint/js": "^9.17.0",
    "bootstrap": "^5.2.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-config-prettier-vue": "^1.0.0",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.14.0",
    "js-cookie": "^3.0.5",
    "prettier": "^3.4.2",
    "typescript-eslint": "^8.19.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.3",
    "@babel/plugin-transform-runtime": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@esbuild/linux-x64": "^0.17.17",
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.4.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/jest": "^29.5.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/test-utils": "^2.3.2",
    "@vue/vue3-jest": "^29.2.3",
    "babel-jest": "^29.5.0",
    "concurrently": "^8.0.1",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "esbuild": "^0.17.16",
    "expect-puppeteer": "^8.0.5",
    "fix-esm-import-path": "^1.3.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-environment-node-single-context": "^29.0.0",
    "jest-image-snapshot": "^6.1.0",
    "jest-puppeteer": "^8.0.6",
    "jest-serializer-vue": "^3.1.0",
    "path": "^0.12.7",
    "postcss": "^8.4.21",
    "postcss-css-variables": "^0.18.0",
    "puppeteer": "^22.12.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "sass": "^1.80.2",
    "sass-true": "^8.1.0",
    "terser": "^5.16.6",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0",
    "tsc-watch": "^6.0.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.2",
    "url": "^0.11.0",
    "vite": "^4.5.3",
    "vite-plugin-vue-markdown": "^0.22.6",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6",
    "vue-tsc": "^2.1.10"
  },
  "main": "./dist/components.umd.js",
  "module": "./dist/components.es.js",
  "files": [
    "dist/components.umd.js",
    "dist/components.es.js",
    "dist/style.css",
    "assets/*",
    "src/*"
  ],
  "exports": {
    ".": {
      "import": "./dist/components.js",
      "require": "./dist/components.umd.cjs"
    }
  },
  "license": "UNLICENSED",
  "registry-url": "https://registry.npmjs.com",
  "repository": {
    "type": "git",
    "url": "git://github.com/iamproperty/iamproperty.github.io.git"
  },
  "localURL": "http://192.168.0.69:5173"
}
